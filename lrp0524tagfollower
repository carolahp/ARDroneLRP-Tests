;;; New program created by CaroHernandez on 25 May 2016(var vel := [0.2])(machine tagFollower	(state waiting)	(state following		(machine followAlgo			(state positioned)			(state moving				(running [robot shellTagMoveAndFollow])				(onexit [robot stabilize]))			(event offCenter [robot shellTagIsCentered not])			(event inCenter [robot shellTagIsCentered])			(on offCenter positioned -> moving)			(on inCenter moving -> positioned)		)		(onentry (spawn followAlgo positioned))	)	(event tagFound [robot shellTagDetected])	(event tagLost [robot shellTagDetected not])	(on tagFound waiting -> following)	(on tagLost following -> waiting))(spawn tagFollower waiting)