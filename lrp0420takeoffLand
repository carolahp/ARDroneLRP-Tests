;;; New program created by CaroHernandez on 20 April 2016(machine drone	(state takingOff		(onentry [robot takeOff]))	(state landing		(onentry [robot land]))	(event tookOff [robot isFlying])	(event landed [robot isFlying not])	(on tookOff takingOff -> landing)	(on landed landing -> takingOff))(spawn drone takingOff)